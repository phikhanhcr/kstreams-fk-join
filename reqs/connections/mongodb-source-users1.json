{
  "name": "mongodb-source-connector-users1",
  "config": {
    "connector.class": "io.debezium.connector.mongodb.MongoDbConnector",
    "tasks.max": "1",
    "mongodb.hosts": "rs0/mymongo:27017",
    "mongodb.name": "name",
    "database.include.list": "backend",
    "snapshot.mode": "initial",
    "signal.data.collection": "backend.signal",
    "behavior.on.null.values": "delete",
    "errors.tolerance": "all",
    "collection.include.list": "backend.customers1,backend.signal",
    "database.history.kafka.bootstrap.servers": "kafka:9092",
    "transforms": "unwrap,route,extractKey,InsertField,InsertField1,InsertField2",
    "transforms.extractKey.type": "org.apache.kafka.connect.transforms.ExtractField$Key",
    "transforms.extractKey.field": "id",
    "transforms.unwrap.type": "io.debezium.connector.mongodb.transforms.ExtractNewDocumentState",
    "transforms.unwrap.array.encoding": "document",
    "transforms.unwrap.delete.handling.mode": "none",
    "transforms.unwrap.drop.tombstone": "true",
    "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.route.regex": "([^.]+)\\.([^.]+)\\.([^.]+)",
    "transforms.route.replacement": "cdc_$3",
    "transforms.InsertField.type": "org.apache.kafka.connect.transforms.InsertField$Value",
    "transforms.InsertField.static.field": "email",
    "transforms.InsertField.static.value": "",
    "transforms.InsertField1.type": "org.apache.kafka.connect.transforms.InsertField$Value",
    "transforms.InsertField1.static.field": "first_name",
    "transforms.InsertField1.static.value": "",
    "transforms.InsertField2.type": "org.apache.kafka.connect.transforms.InsertField$Value",
    "transforms.InsertField2.static.field": "last_name",
    "transforms.InsertField2.static.value": ""
  }
}